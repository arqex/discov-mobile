{
  "name": "@discov/mobile",
  "packagename": "discovmobile",
  "version": "1.3.0",
  "scripts": {
    "start": "react-native start",
    "start:local": "expo start --host localhost --config app-local.json",
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "android:bundle": "cd android && ./gradlew assembleRelease",
    "android:ota:Production": "appcenter codepush release-react -a arqex/discov-android -d Production",
    "android:bgFetch": "adb shell cmd jobscheduler run -f com.discovmobile 999",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --device --configuration Release",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "bumpMinor": "node ./scripts/bumpVersion.js minor",
    "bumpMajor": "node ./scripts/bumpVersion.js major"
  },
  "dependencies": {
    "@aws-amplify/api": "^3.1.19",
    "@aws-amplify/auth": "^3.3.1",
    "@aws-amplify/core": "^3.4.2",
    "@aws-amplify/pubsub": "^3.0.20",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/google-signin": "^4.0.3",
    "@react-native-community/push-notification-ios": "1.1.0",
    "@react-native-community/slider": "^2.0.9",
    "amazon-cognito-identity-js": "^4.3.3",
    "appcenter": "3.0.3",
    "appcenter-analytics": "3.0.3",
    "appcenter-crashes": "3.0.3",
    "expo": "^37.0.0",
    "expo-location": "^8.1.0",
    "expo-task-manager": "~8.1.0",
    "haversine": "^1.1.1",
    "i18n-js": "^3.7.0",
    "js-base64": "^2.5.2",
    "memoize-one": "^5.1.1",
    "onstate": "^0.7.4",
    "react": "16.13.1",
    "react-interactable": "^0.6.4",
    "react-native": "0.61.5",
    "react-native-background-fetch": "^3.1.0",
    "react-native-code-push": "^6.2.1",
    "react-native-device-info": "^5.5.4",
    "react-native-fast-image": "^8.1.5",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-image-crop-picker": "^0.32.2",
    "react-native-localize": "^1.4.0",
    "react-native-maps": "0.26.1",
    "react-native-push-notification": "^4.0.0",
    "react-native-svg": "11.0.1",
    "react-native-unimodules": "~0.8.1",
    "urlstack": "^0.7.11"
  },
  "devDependencies": {
    "@babel/core": "~7.6.0",
    "babel-jest": "~24.9.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "~0.56.0",
    "react-test-renderer": "~16.9.0",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "babel-preset-expo": "^8.1.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.8.3"
  },
  "private": true
}
